{
  "name": "creative-agency",
  "version": "1.0.0",
  "private": true,
  "description": "Agency Creative Page",
  "type": "module",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\"",
    "eslint": "eslint source/js/",
    "test": "npm run editorconfig && npm run stylelint && npm run eslint",
    "build": "gulp buildProd",
    "start": "gulp",
    "deploy": "npm run build && gulp deploy"
  },
  "browserslist": [
    "> 0.5%",
    "last 2 versions",
    "Firefox ESR",
    "not dead"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@babel/preset-env": "^7.20.2",
    "@htmlacademy/editorconfig-cli": "^1.0.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^9.1.0",
    "browser-sync": "^2.27.10",
    "del": "^7.0.0",
    "eslint": "^8.29.0",
    "eslint-config-htmlacademy": "^8.0.0",
    "gh-pages": "^5.0.0",
    "gulp": "^4.0.2",
    "gulp-csso": "^4.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^9.0.1",
    "gulp-posthtml": "^3.0.5",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "^5.1.0",
    "gulp-svgstore": "^9.0.0",
    "postcss": "^8.4.19",
    "postcss-custom-properties": "^12.1.11",
    "postcss-scss": "^4.0.6",
    "posthtml-include": "^1.7.4",
    "sass": "^1.56.1",
    "stylelint": "^14.16.0",
    "stylelint-config-htmlacademy": "^1.1.4",
    "webpack": "^5.75.0",
    "webpack-stream": "^7.0.0"
  },
  "dependencies": {
    "core-js": "^3.26.1"
  }
}
